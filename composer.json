{
  "require": {
    "codemonauts/craft-instagram-feed": "1.0.5",
    "craftcms/cms": "3.4.17.1",
    "craftcms/contact-form": "2.2.6",
    "craftcms/contact-form-honeypot": "1.0.2",
    "craftcms/redactor": "2.6.1",
    "ether/seo": "3.6.4",
    "fortrabbit/craft-copy": "1.0.0-RC10",
    "misterbk/mix": "1.5.2",
    "sebastianlenz/linkfield": "1.0.21",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
